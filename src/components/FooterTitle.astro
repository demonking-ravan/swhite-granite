---
interface Props {
    text: string
}

const {text} = Astro.props
---
<div class="flex items-center gap-3 mt-3.5 mb-5">
    <div class="w-3.5 h-3.5 bg-[#8A5133] relative before:absolute before:top-0 before:left-0 before:w-2 before:h-2 before:bg-primary-500"></div>
    <h3 class="text-lg font-semibold font-primary tracking-wide text-white">{text}</h3>
</div>